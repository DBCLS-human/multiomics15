{
  "@context": {
    "stanza": "http://togostanza.org/resource/stanza#"
  },
  "@id": "snv",
  "stanza:label": "SNV",
  "stanza:definition": "Displaying alleles in the specified range.",
  "stanza:parameter": [
      {
	  "stanza:key": "sample",
	  "stanza:example": "TSE000086, LC2/ad",
	  "stanza:description": "Example name or ID",
	  "stanza:required": true
      },
      {
	  "stanza:key": "chromosome",
	  "stanza:example": "1",
	  "stanza:description": "Chromosome number",
	  "stanza:required": true
      },
      {
	  "stanza:key": "range_start",
	  "stanza:example": "10000",
	  "stanza:description": "The start of the range",
	  "stanza:required": true
      },
      {
	  "stanza:key": "range_end",
	  "stanza:example": "11000",
	  "stanza:description": "The end of the range",
	  "stanza:required": true
      }                  
  ],
  "stanza:usage": "<togostanza-snv sample=\"LC2/ad\" chromosome=\"1\" range_start=\"10000\" range_end=\"11000\"></togostanza-snv>",
